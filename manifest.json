{
    "manifest_version": 2,
    "name": "Gmail Attachment Management",
    "version": "1.0",
    "description": "管理你的gmail附件",

    "permissions": [ 
      "*://*.google.com/*",
      "*://mail.google.com/*" 
    ],

    "content_scripts": [
        {
            "matches": ["*://mail.google.com/*"],
            "js": ["js/jquery-1.11.1.min.js","js/addBtn.js"]
        }
    ]
}